@charset "utf-8";
#container{
	width: 100%;
	border-style:none;
	


}
#header{
	width:85%;
	
}
#header h1{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 60px;
	color: white;
	text-align:center;
	background-color: blue;
	border: white;
	margin-left: auto;
	margin-right: auto;



}
#content{
	font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;




}
#side {
	width:15%;
	vertical-align:top;
	border-right:1px solid;


}
#sidebar{
	border:1px solid;
	margin:10px;
	width:12%;
	padding:10px;
	position:fixed;
	top:0;
	left:0;
}

body{
	Background-color:rgb(236,162,73);
	
	
	
	
	
}
.infographic {
	display: block;
	margin-left: auto;
	margin-right: auto;
}
.boxed {
	border: 1px solid white;
	width: 500px;
	background-color: blue;
}